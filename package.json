{
  "name": "react-redux-todo-list",
  "description": "React, Redux, Brunch and Jest TodoList app",
  "author": "Junior Ales",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/junior-ales/react-redux-todo-list"
